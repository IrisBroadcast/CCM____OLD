@model IEnumerable<CCM.Core.Entities.RadiusUser>

@{
    ViewBag.Title = "GetRadiusUsers";
}

<h2>GetRadiusUsers</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Username)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Username)
        </td>
    </tr>
}

</table>
